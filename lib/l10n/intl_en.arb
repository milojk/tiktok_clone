{
  "@@locale": "en",
  "signUpTitle": "Sign up for {nameOfTheApp} {when}",
  "@signUpTitle": {
    "description": "The title that people see when they open the app for the first time.",
    "placeholders": {
      "nameOfTheApp": {
        "type": "String",
        "example": "TikTok"
      },
      "when": {
        "type": "DateTime",
        "format": "y / QQQQ / LLLL : Hm",
        "isCustomDateFormat" : "true"
      }
    }
  },
  "loginTitle": "Log into your {nameOfTheApp} account",
  "@loginTitle": {
    "description": "The title that people see when they login the app.",
    "placeholders": {
      "nameOfTheApp": {
        "type": "String",
        "example": "TikTok"
      }
    }
  },
  "signUpSubtitle": "Create a profile, follow other accounts, make your own {videoCount, plural, =0{no videos} =1{video} other{videos}}, and more.",
  "emailPasswordButton": "Use email & password",
  "appleButton": "Continue with Apple",
  "alreadyHaveAnAccount": "Already have an account?",
  "logIn": "Log In {gender, select, male{sir} female{madam} other{human}}",
  "likeCount": "{potato}",
  "@likeCount": {
    "description": "The number of likes pressed by users.",
    "placeholders": {
      "potato": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "commentCount": "{potato}",
  "@commentCount": {
    "description": "The number of comments made by users.",
    "placeholders": {
      "potato": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "commentTitle": "{value} {value2, plural ,  =1{comment} other{comments}}",
  "@commentTitle": {
    "description": "The number of comments made by users.",
    "placeholders": {
      "value": {
        "type": "int",
        "format": "compact"
      }
    }
  }
}